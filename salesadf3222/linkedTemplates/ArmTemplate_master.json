{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "salesadf3222"
		},
		"ls_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks'"
		},
		"ls_onprem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_onprem'"
		},
		"ls_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0422-162902-uv324qq0"
		},
		"ls_onprem_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "SHAH\\MSSQLSERVER2"
		},
		"ls_onprem_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "retailsales"
		},
		"ls_onprem_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"ls_restservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"ls_storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://retailsalesstorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "salesadf3222_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_databricks_accessToken": {
						"value": "[parameters('ls_databricks_accessToken')]"
					},
					"ls_onprem_password": {
						"value": "[parameters('ls_onprem_password')]"
					},
					"ls_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_restservice_properties_typeProperties_url": {
						"value": "[parameters('ls_restservice_properties_typeProperties_url')]"
					},
					"ls_storage_properties_typeProperties_url": {
						"value": "[parameters('ls_storage_properties_typeProperties_url')]"
					},
					"ls_onprem_properties_typeProperties_server": {
						"value": "[parameters('ls_onprem_properties_typeProperties_server')]"
					},
					"ls_onprem_properties_typeProperties_database": {
						"value": "[parameters('ls_onprem_properties_typeProperties_database')]"
					},
					"ls_onprem_properties_typeProperties_userName": {
						"value": "[parameters('ls_onprem_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}